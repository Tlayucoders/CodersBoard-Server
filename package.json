{
  "name": "codersBoard",
  "version": "1.0.0",
  "main": "index.js",
  "author": "esau.opr@gmail.com",
  "license": "MIT",
  "scripts": {
    "start": "npm run babel-node ./index.js",
    "develop": "nodemon --exec npm run babel-node ./index.js",
    "babel-node": "babel-node --ignore='node_modules'",
    "build": "babel . --out-dir dist --ignore node_modules,dist,storage,.git,.gitignore --source-maps --copy-files",
    "dev": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --watch --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "gdev": "cross-env NODE_ENV=development gulp build",
    "gwatch": "cross-env NODE_ENV=development gulp watch",
    "gprod": "cross-env NODE_ENV=production gulp build",
    "token": "node ./utils/tokenGenerator.js",
    "docs": "apidoc -i ./app/http/controllers -o ./apidoc/"
  },
  "babel": {
    "presets": [
      "env"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "dependencies": {
    "ajv": "^4.11.5",
    "cross-env": "^3.2.4",
    "dotenv": "^4.0.0",
    "font-awesome": "^4.7.0",
    "jsonwebtoken": "^7.3.0",
    "kcors": "2",
    "koa": "^2.2.0",
    "koa-bodyparser": "^4.1.0",
    "koa-ejs": "^4.0.0",
    "koa-jwt": "^3.0.0",
    "koa-router": "next",
    "koa-static": "^3.0.0",
    "mongorito": "^2.2.0",
    "vue": "^2.2.4",
    "vue-chartjs": "^2.5.4",
    "vue-localstorage": "^0.1.3",
    "vue-resource": "^1.2.1",
    "vue-router": "^2.3.0",
    "vuetify": "^0.9.2",
    "vuex": "^2.2.1",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "apidoc": "^0.17.5",
    "babel-cli": "^6.24.0",
    "babel-eslint": "6",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.2.2",
    "eslint": "3.x",
    "gulp": "^3.9.1",
    "gulp-notify": "^3.0.0",
    "gulp-sourcemaps": "^2.4.1",
    "gulp-stylus": "^2.6.0",
    "laravel-mix": "^0.8.9",
    "nodemon": "^1.11.0",
    "rollup": "^0.41.6",
    "rollup-plugin-alias": "^1.2.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-multi-entry": "^2.0.1",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-typescript": "^0.8.1",
    "rollup-plugin-vue": "^2.3.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1"
  }
}
